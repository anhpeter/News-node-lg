<% 
    var title = title || 'Online';
%>  
<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title"><%= title %></h3>
        <span id="total-users-online" data-toggle="tooltip" title="" class="badge bg-light-blue count">0</span>
        <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i
                    class="fa fa-minus"></i>
            </button>
        </div>
    </div>
    <!-- /.box-header -->
    <div class="box-body users-online-box">
    </div>
    <!-- /.box-body -->
</div>